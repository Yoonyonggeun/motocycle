extends main.pug
include mixins/videoBlock.pug

block content
  .search__header
    h1 Searching by #{searching}
  .search__video
    if videos.length === 0
      h5 No Videos Found
    each item in videos
      +videoBlock({
        title:item.title,
        views:item.views,
        videoFile:item.videoFile
       })
